{
  "name": "@shopware-ag/gh-project-automation",
  "version": "1.4.2",
  "description": "Project automation for shopware",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "prepack": "unbuild",
    "lint": "eslint .",
    "cli": "npx tsx ./src/scripts/cli.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "unbuild": {
    "clean": true,
    "sourcemap": true
  },
  "keywords": [
    "shopware",
    "github",
    "actions"
  ],
  "author": "shopware AG",
  "license": "MIT",
  "optionalDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/glob": "^0.5.0",
    "@actions/io": "^1.1.3",
    "@types/node-fetch": "^2.6.12",
    "node-fetch": "^2.6.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "unbuild": "^3.5.0"
  }
}
